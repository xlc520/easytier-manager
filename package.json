    "electron-updater": "^6.3.9",
    "element-plus": "2.8.6",
    "intro.js": "^7.2.0",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "monaco-editor": "^0.52.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.4",
    "pinia-plugin-persistedstate": "^3.2.3",
    "qrcode": "^1.5.4",
    "qs": "^6.13.0",
